    "react-dom": "^17.0.1",
    "react-fontawesome": "^1.7.1",
    "react-perfect-scrollbar": "^1.5.3",
    "react-redux": "^7.1.3",
    "react-scripts": "4.0.1",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "sip.js": "^0.7.8",
    "socket.io-client": "^2.3.0",
    "styled-components": "^5.2.1",
    "yup": "^0.29.3"
  },
  "husky": {
