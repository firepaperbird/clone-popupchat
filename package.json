{
  "name": "live-chat-widget",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "npm run build:css && webpack serve --env NODE_ENV=local",
    "build:production": "npm run build:css && webpack --mode production --env NODE_ENV=production --progress --profile",
    "build:development": "npm run build:css && webpack --mode production --env NODE_ENV=development --progress --profile",
    "deploy:configure": "aws configure",
    "deploy:production": "aws s3 cp dist s3://eg-production-chatchilla-live-chat-origin/ --cache-control max-age=0 --recursive",
    "deploy:development": "aws s3 cp dist s3://chatchilla-live-chat-plugin-dev/ --cache-control max-age=0 --recursive",
    "invalidate:development": "aws configure set preview.cloudfront true && aws cloudfront create-invalidation --distribution-id E35DIW48OIP1RM --paths '/*'", 
    "invalidate:production": "aws configure set preview.cloudfront true && aws cloudfront create-invalidation --distribution-id E26JB6RPEG6ZF0 --paths '/*'", 
    "check-type": "tsc -w",
    "build:css": "gulp build-namespaced-css"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/preset-env": "^7.12.10",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@types/lodash": "^4.14.165",
    "@types/node": "^14.14.13",
    "@types/qs": "^6.9.5",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.12",
    "@types/sip.js": "^0.12.0",
    "@types/socket.io-client": "^1.4.34",
    "@types/styled-components": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-import": "^1.13.3",
    "clean-webpack-plugin": "^3.0.0",
    "core-js": "^3.8.1",
    "css-loader": "^5.0.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-standard": "^5.0.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^6.0.8",
    "html-webpack-plugin": "^4.5.0",
    "husky": ">=4",
    "lint-staged": ">=10",
    "prettier": "^2.2.1",
    "regenerator-runtime": "^0.13.7",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "style-loader": "^2.0.0",
    "svg-url-loader": "^7.1.1",
    "terser-webpack-plugin": "^5.0.3",
    "typescript": "^4.1.3",
    "webpack": "^5.10.1",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@fingerprintjs/fingerprintjs": "^3.0.4",
    "antd": "^4.9.4",
    "axios": "^0.19.0",
    "emoji-mart": "^3.0.0",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-postcss": "^9.0.0",
    "gulp-rename": "^2.0.0",
    "lodash": "^4.17.20",
